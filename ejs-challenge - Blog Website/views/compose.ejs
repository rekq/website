<%- include('partials/header'); -%>
    <h1>Compose</h1>
    <form class="item" action="/compose" method="post">
        <div class="form-group">
            <label for="">Title</label>
            <input type="text" class="form-control" name="postTitle" autocomplete="off">
        </div>
        <div class="form-group">
            <label for="">Post</label>
            <textarea name="postBody" class="form-control" rows="5" id="comment"></textarea>
        </div>
        <button type="submit" class="btn btn-primary">Publish</button>
    </form>
<%- include('partials/footer'); -%>